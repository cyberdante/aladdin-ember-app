{{#paper-card as |card|}}
    {{#if useTitle}}
        {{#card.title as |title|}}
            {{#title.text as |text|}}
                {{#text.headline}}Graph View{{/text.headline}}
            {{/title.text}}
        {{/card.title}}
    {{/if}}
    {{#card.content}}
      {{svg}}
    {{/card.content}}
{{/paper-card}}