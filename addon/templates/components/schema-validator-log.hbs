{{#paper-card as |card|}}
    {{#card.title as |title|}}
        {{#title.text as |text|}}
            {{#text.headline}}Log{{/text.headline}}
        {{/title.text}}
    {{/card.title}}
    {{#card.content}}
        {{#paper-list}}
            {{#each outputs as |output|}}
                {{#paper-item class="md-1-line"}}
                    <div class="md-list-item-text">
                        {{schema-validator-log-line output=output}}
                    </div>
                {{/paper-item}}
            {{/each}}
        {{/paper-list}}
    {{/card.content}}
    {{#if (and outputs outputs.length)}}
        {{paper-divider}}
        {{#card.actions}}
            {{#paper-item class="md-1-line"}}
                <div class="md-list-item-text bold-text">
                    {{outputs.length}} errors and/or warnings found
                </div>
            {{/paper-item}}
        {{/card.actions}}
    {{/if}}
{{/paper-card}}